<%= render "admin_side", :current => "invoices" %>

<div class="contenter">
  <div class="section">
    <h1>Taxes</h1>
  </div>
  <div class="section">
    <table>
      <tr>
        <th>ID</th>
        <th>User ID</th>
      </tr>
      <% @invoices.each do |i| %>
        <tr>
          <td><%= i.id %></td>
          <td><%= i.user_id %></td>
        </tr>
      <% end %>
    </table>
  </div>
</div>